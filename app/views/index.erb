<!DOCTYPE html>
<html>
<head>
  <script src="application.js"></script>
<script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyDkelbKDQ5vdNiJluSOxCcMthP8pDiXppM&sensor=false">
</script>



<script>
var myCenter=new google.maps.LatLng(41.8500,-87.6500);
var marker;

var myCenter2=new google.maps.LatLng(21.6536,-157.9273);




function initialize()
{
var mapProp = {
  center:myCenter,
  zoom:8,
  mapTypeId:google.maps.MapTypeId.ROADMAP
  };

var mapProp2 = {
  center:myCenter2, 
  zoom:10,
  myTypeId:google.maps.MapTypeId.HYBRID
}

var mapProp3 = {
  center:myCenter2, 
  zoom:10, 
  myTypeId:google.maps.MapTypeId.TERRAIN
}



var styles = [
  {
    stylers: [
       // #00ffe6
              
      { hue: "#eeeeee" },
      { saturation: -55 }
    ]
  },{
    featureType: "road",
    elementType: "geometry",
    stylers: [
      { lightness: 100 },
      { visibility: "simplified" }
    ]
  },{
    featureType: "road",
    elementType: "labels",
    stylers: [
      { visibility: "off" }
    ]
  }
];

var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
var map2= new google.maps.Map(document.getElementById("googleMap2"),mapProp2);
var map3= new google.maps.Map(document.getElementById("googleMap3"),mapProp3); 


map.setOptions({styles: styles});
map2.setOptions({styles: styles}); 
map3.setOptions({styles: styles}); 




marker=new google.maps.Marker({
  position:myCenter,
  animation:google.maps.Animation.BOUNCE
  });

marker.setMap(map);
}

google.maps.event.addDomListener(window, 'load', initialize);
</script>
</head>

<body>

<div class="container">
  <h1> G-Maps Gettin' G-Crazy</h1>
<h2>My fav places</h2>
<div id="googleMap" class="container" style="width:500px;height:380px;"></div>
<h2>My fav places</h2>
<div id="googleMap2" class="container" style="width:500px;height:380px;"></div>
<div id="googleMap3" class="container" style="width:500px;height:380px;"></div>

</div>
</body>
</html>